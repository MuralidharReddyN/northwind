{
	"name": "SQL_2_ADLS_Categories",
	"properties": {
		"description": "Ingest Categories from SQL Server to Storage Account",
		"activities": [
			{
				"name": "SQL_2_ADLS_Categories",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "dbo.Categories"
					},
					{
						"name": "Destination",
						"value": "northwind/ingestion/masterdata/sales/"
					}
				],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": false
				},
				"inputs": [
					{
						"referenceName": "src_Categories",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "trg_Categories",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}